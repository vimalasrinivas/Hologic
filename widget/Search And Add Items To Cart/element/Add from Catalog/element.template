<div id="addFromCatalog">
  <button class="cc-button-secondary"  style="margin-bottom:1rem;" data-bind="click: addFromCatalog.bind($data),
  disable:($data.cart() && $data.cart().currentOrderState() == 'QUOTED')">
    <span class="glyphicon glyphicon-plus"></span>
    <span data-bind="widgetLocaleText:'addFromCatalogText'"></span>
  </button>
</div>